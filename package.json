{
  "name": "clean-self-webpack-plugin",
  "version": "2.0.1",
  "description": "Webpack plugin to delete extraneous files created by Webpack",
  "keywords": [
    "webpack",
    "plugin",
    "clean",
    "cleanup",
    "clear",
    "directory",
    "file",
    "exclude",
    "remove",
    "delete",
    "trash",
    "rm",
    "rf",
    "rmrf",
    "rimraf",
    "del",
    "node"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisblossom/clean-self-webpack-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/chrisblossom/clean-self-webpack-plugin/issues"
  },
  "author": "Chris Blossom",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.10",
    "yarn": ">=1.6.0"
  },
  "devEngines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/clean-self-webpack-plugin.js",
  "scripts": {
    "build": "backtrack build --production",
    "check-all": "backtrack check-all",
    "clean": "backtrack clean",
    "dev": "backtrack dev --development",
    "format": "backtrack format",
    "lint": "backtrack lint",
    "lint.fix": "backtrack lint.fix",
    "precommit": "backtrack precommit",
    "prepublishOnly": "backtrack prepublishOnly",
    "prepush": "backtrack prepush",
    "test": "backtrack test",
    "test.all": "backtrack test.all",
    "test.ci": "backtrack test.ci",
    "test.ci-pretest": "backtrack test.ci-pretest",
    "test.update": "jest --updateSnapshot",
    "test.webpack2": "backtrack test.webpack2",
    "test.webpack3": "backtrack test.webpack3",
    "test.webpack4": "backtrack test.webpack4"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "./.babelrc.js"
    ]
  },
  "devDependencies": {
    "@backtrack/core": "^0.1.17",
    "@backtrack/preset-node-module": "^0.1.11",
    "webpack": "^4.8.3"
  },
  "dependencies": {
    "del": "^3.0.0"
  }
}
